on boot
    write /sys/class/android_usb/android0/iSerial 1234567890
    write /sys/class/android_usb/android0/iManufacturer NVIDIA
    write /sys/class/android_usb/android0/iProduct jetson-tk1 

    write /sys/class/android_usb/android0/f_rndis/manufacturer NVIDIA
    write /sys/class/android_usb/android0/f_rndis/vendorID 0955
    write /sys/class/android_usb/android0/f_rndis/wceis 1
    setprop sys.usb.config mtp,adb

on fs
	mkdir /dev/usb-ffs 0770 shell shell
	mkdir /dev/usb-ffs/adb 0770 shell shell
	mount functionfs adb /dev/usb-ffs/adb uid=2000,gid=2000
	write /sys/class/android_usb/android0/f_ffs/aliases adb

on property:sys.usb.config=mtp
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor ${ro.nv.usb.vid}
    write /sys/class/android_usb/android0/idProduct ${ro.nv.usb.pid.mtp}
    write /sys/class/android_usb/android0/functions ${sys.usb.config}
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=mtp,adb
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 0955
    write /sys/class/android_usb/android0/idProduct 7100
    write /sys/class/android_usb/android0/functions mtp,adb
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state mtp,adb

on property:sys.usb.config=rndis
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor ${ro.nv.usb.vid}
    write /sys/class/android_usb/android0/idProduct ${ro.nv.usb.pid.rndis}
    write /sys/class/android_usb/android0/functions ${sys.usb.config}
    write /sys/class/android_usb/android0/bDeviceClass 224
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=rndis,adb
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor ${ro.nv.usb.vid}
    write /sys/class/android_usb/android0/idProduct ${ro.nv.usb.pid.rndis.adb}
    write /sys/class/android_usb/android0/functions ${sys.usb.config}
    write /sys/class/android_usb/android0/bDeviceClass 0
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=ptp
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor ${ro.nv.usb.vid}
#TODO: should this be a new product #
    write /sys/class/android_usb/android0/idProduct ${ro.nv.usb.pid.ptp}
    write /sys/class/android_usb/android0/functions ${sys.usb.config}
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=ptp,adb
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor ${ro.nv.usb.vid}
#TODO: should this be a new product #
    write /sys/class/android_usb/android0/idProduct ${ro.nv.usb.pid.ptp.adb}
    write /sys/class/android_usb/android0/functions ${sys.usb.config}
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state ${sys.usb.config}
    
